{
  "name": "whisper-provider",
  "version": "1.0.0",
  "description": "Dynamic Whisper provider for MediaConduit - OpenAI Whisper speech-to-text processing via Docker",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "clean": "rimraf dist",
    "test": "jest",
    "lint": "eslint src/**/*.ts",
    "prepublishOnly": "npm run clean && npm run build"
  },
  "keywords": [
    "mediaconduit",
    "whisper",
    "speech-to-text",
    "stt",
    "audio",
    "transcription",
    "docker",
    "provider",
    "dynamic"
  ],
  "author": "MediaConduit",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/MediaConduit/whisper-provider.git"
  },
  "bugs": {
    "url": "https://github.com/MediaConduit/whisper-provider/issues"
  },
  "homepage": "https://github.com/MediaConduit/whisper-provider#readme",
  "dependencies": {
    "axios": "^1.6.0",
    "form-data": "^4.0.0"
  },
  "devDependencies": {
    "@mediaconduit/mediaconduit": "^0.1.2",
    "@types/node": "^20.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.0.0",
    "jest": "^29.0.0",
    "rimraf": "^5.0.0",
    "typescript": "^5.0.0"
  },
  "peerDependencies": {
    "@mediaconduit/mediaconduit": ">=1.0.0"
  },
  "mediaconduit": {
    "minimumVersion": "1.0.0",
    "compatibility": "1.x",
    "type": "provider",
    "capabilities": [
      "audio-to-text",
      "speech-to-text",
      "transcription",
      "translation"
    ]
  }
}
